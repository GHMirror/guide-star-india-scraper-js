an>\r\n                    </b>\r\n                </td>\r\n\t\t<td class=\"GSBorderTableItemRow\">\r\n                    <span id=\"SectionPlaceHolder1_ctl01_TextControl35\"><div class=\"CPBData\">\r\n\t\t\t<div>\r\n\r\n\t\t\t</div><span id=\"SectionPlaceHolder1_ctl01_TextControl35_desc\"></span><span>302.4</span>&nbsp;<span></span>\r\n\t\t</div></span>\r\n                </td>\r\n\t</tr>\r\n\t<tr>\r\n\t\t<td class=\"GSBorderTableItemRow fintitle\">\r\n                    <b>\r\n                        <span id=\"SectionPlaceHolder1_ctl01_TextPlaceHolder17\">Total</span>\r\n                        &nbsp;<span id=\"SectionPlaceHolder1_ctl01_TextControl37\" class=\"InlineBlock\"><div>\r\n\t\t\t<div class=\"CTPParaClippingHeaderLabel\">\r\n\t\t\t\t<span id=\"SectionPlaceHolder1_ctl01_TextControl37_lbl\">Expenditure</span>\r\n\t\t\t</div><span id=\"SectionPlaceHolder1_ctl01_TextControl37_desc\"></span><span></span>&nbsp;<span></span>\r\n\t\t</div></span>\r\n                    </b>\r\n                </td>\r\n\t\t<td class=\"GSBorderTableItemRow\">\r\n                    <span id=\"SectionPlaceHolder1_ctl01_TextControl38\"><div class=\"CPBData\">\r\n\t\t\t<div>\r\n\r\n\t\t\t</div><span id=\"SectionPlaceHolder1_ctl01_TextControl38_desc\"></span><span>331.3</span>&nbsp;<span></span>\r\n\t\t</div></span>\r\n                </td>\r\n\t</tr>\r\n\t<tr>\r\n\t\t<td class=\"GSBorderTableItemRow fintitle\">\r\n                    <b>\r\n                        <span id=\"SectionPlaceHolder1_ctl01_TextPlaceHolder18\">Total</span>\r\n                        &nbsp;<span id=\"SectionPlaceHolder1_ctl01_TextControl50\" class=\"InlineBlock\"><div>\r\n\t\t\t<div class=\"CTPParaClippingHeaderLabel\">\r\n\t\t\t\t<span id=\"SectionPlaceHolder1_ctl01_TextControl50_lbl\">Receipts</span>\r\n\t\t\t</div><span id=\"SectionPlaceHolder1_ctl01_TextControl50_desc\"></span><span></span>&nbsp;<span></span>\r\n\t\t</div></span>\r\n                    </b>\r\n                </td>\r\n\t\t<td class=\"G